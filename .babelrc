{
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"uglify": true
				},
				"modules": false
			}
		],
		"@babel/react"
	],
	"plugins": [
		"transform-class-properties",
		"transform-object-rest-spread",
		"syntax-dynamic-import"
	],
	"env": {
		"test": {
			"presets": ["@babel/env", "@babel/react"],
			"plugins": [
				"system-import-transformer",
				"transform-class-properties",
				"transform-object-rest-spread"
			]
		}
	}
}
